{
	"name": "mypass",
	"description": "Chrome integration for \"mypass\" password manager",
	"version": "1.2",
	"manifest_version": 2,
	"minimum_chrome_version": "33",
	"permissions": [
		"http://*/*",
		"https://*/*",
		"nativeMessaging",
		"tabs"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [
		{
			"js": ["content.js"],
			"all_frames": true,
			"matches": ["http://*/*", "https://*/*"]
		}
	],
	"page_action": {}
}
